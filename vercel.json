{
    "version": 2,
    "builds": [
      {
        "src": "src/index.ts",
        "use": "@vercel/node"
      },
      {
        "src": "src/public/**",
        "use": "@vercel/static"
      }
    ],
    "routes": [
      {
        "src": "/js/(.*)",
        "dest": "/src/public/js/$1"
      },
      {
        "src": "/css/(.*)",
        "dest": "/src/public/css/$1"
      },
      {
        "src": "/api/(.*)",
        "dest": "src/index.ts"
      },
      {
        "src": "/(.*)\\.html",
        "dest": "/src/public/$1.html"
      },
      {
        "src": "/(login|register|dashboard|.*)",
        "dest": "src/index.ts"
      }
    ]
  }